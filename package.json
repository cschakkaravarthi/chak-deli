{
  "name": "umgc-web",
  "version": "5.1.0",
  "private": true,
  "description": "A corporate intranet for the web.",
  "umgcApi": {
    "version": "5.0.2",
    "useEdge": true
  },
  "scripts": {
    "env:local": "NODE_ENV=local node scripts/config.js && cp src/config/index.local.js src/config/index.js",
    "env:dev": "NODE_ENV=dev node scripts/config.js",
    "env:qa": "NODE_ENV=qa node scripts/config.js",
    "env:uat": "NODE_ENV=uat node scripts/config.js",
    "env:prod": "NODE_ENV=prod node scripts/config.js",
    "allenvs": "npm run env:qa && npm run env:uat && npm run env:prod && npm run env:dev",
    "start": "npm run env:local && webpack-dev-server --mode development --open --hot",
    "start:production": "npm run build && node server.js | node --http-parser=legacy gso-transport.js",
    "build": "npm run allenvs && webpack --mode production && cp src/400.html dist/",
    "build:docker": "docker build . --build-arg ENV=${ENV} --build-arg NODE_ENV=${NODE_ENV} --build-arg VAULT_ADDR=${VAULT_ADDR} --build-arg VAULT_METHOD=${VAULT_METHOD} --build-arg VAULT_USER=$VAULT_USER --build-arg VAULT_PASS=$VAULT_PASS",
    "start:docker": "echo Enter your AD Username && read -s VAULT_USER && echo Enter your AD Password && read -s VAULT_PASS && docker build . --build-arg ENV=local --build-arg NODE_ENV=development --build-arg VAULT_ADDR=https://vault.umusic.net --build-arg VAULT_METHOD=ldap --build-arg VAULT_USER=$VAULT_USER --build-arg VAULT_PASS=$VAULT_PASS",
    "test": "npm run lint && jest --coverage",
    "test:fix": "npm run lint:fix && jest --coverage -u",
    "versionfile": "git log --pretty=format:'%h' -n 1 > version.txt",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore",
    "lint:fix": "eslint . --fix --ext .ts,.tsx --ignore-path .gitignore",
    "precommit": "lint-staged",
    "pull:ui": "npm i --registry https://artifactory.umusic.net/artifactory/api/npm/umg-npm/"
  },
